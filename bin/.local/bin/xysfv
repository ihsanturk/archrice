#!/bin/bash

# xysfv - Extract YouTube Auto Subtitle From a Video

URL=''
lang=en
fn=$1

# youtube-dl --write-auto-sub --sub-lang $lang --skip-download $URL

sed -i.bak -E -e '/(^[0-9])|<c>/d' $fn # Delete lines that starts with a
																			 # number or contains <c>.

awk '!x[$0]++' $fn | awk NF | tr '\n' ' ' | fold -s > tmp 
