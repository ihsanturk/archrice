! hard contrast: *background: #1d2021
*background: #0a0a0a
! *background: #212226
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #212226
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
! *color2:  #98971a
*color2:  #77bb00
! *color10: #b8bb26
*color10: #cfdf11
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

URxvt.cursorColor: #bbccbb

! urxvt*fading: 3
urxvt*scrollBar: false
! urxvt*transparent: true
! urxvt*shading: 20

URxvt.perl-ext-common: ...,font-size
Xft*antialias: True

URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt*font: xft:DejaVuSansMono Nerd Font:pixelsize=16:antialias=true:hinting=true

URxvt*boldFont: xft:DejaVuSansMono Nerd Font:style=Bold:bold:pixelsize=16:antialias=true:hinting=true
